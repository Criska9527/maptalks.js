{
  "root": true,
  "env": {
    "es6": true
  },
  "extends": [
    "maptalks"
  ],
  "parser": "babel-eslint",
  "parserOptions": {
    "sourceType": "module"
  },
  "globals": {
    "importScripts" : true
  },
  "rules": {
    "no-var" : "error",
    "prefer-const": "error",
    "global-require": 0
  }
}
