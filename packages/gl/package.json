{
  "name": "@maptalks/gl",
  "version": "0.77.0",
  "description": "WebGL infrastrcture for maptalks",
  "main": "dist/maptalksgl.js",
  "browser": "dist/maptalksgl.js",
  "module": "dist/maptalksgl.mjs",
  "scripts": {
    "build": "npm run lint && cd ../reshader.gl && npm run build && cd ../gl && rollup --environment BUILD:production -c rollup.config.js",
    "build-dev": "cd ../reshader.gl && npm run build-dev && cd ../gl && rollup -c rollup.config.js",
    "dev": "cd ../reshader.gl && npm run build-dev && cd ../gl && rollup -w -c rollup.config.js",
    "test": "karma start --single-run",
    "tdd": "karma start --no-single-run",
    "prepare": "npm run build",
    "lint": "eslint src/**/*.js",
    "pretest": "npm run lint && npm run build",
    "preversion": "npm test"
  },
  "files": [
    "dist/maptalksgl.js",
    "dist/maptalksgl.mjs",
    "dist/transcoders.js"
  ],
  "keywords": [
    "webgl",
    "maptalks"
  ],
  "author": "fuzhenn",
  "license": "UNLICENSED",
  "dependencies": {
    "@mapbox/martini": "^0.2.0",
    "@maptalks/fusiongl": "^0.5.0",
    "@maptalks/regl": "^3.2.3",
    "@maptalks/reshader.gl": "^0.77.0",
    "color": "^3.0.0",
    "gl-matrix": "^2.6.1"
  },
  "devDependencies": {
    "@babel/eslint-parser": "^7.15.0",
    "@babel/preset-env": "^7.15.0",
    "@rollup/plugin-commonjs": "^20.0.0",
    "@rollup/plugin-node-resolve": "^13.0.4",
    "eslint": "^7.32.0",
    "eslint-plugin-mocha": "^9.0.0",
    "expect.js": "^0.3.1",
    "karma": "^6.4.0",
    "karma-chrome-launcher": "^3.1.1",
    "karma-mocha": "^2.0.1",
    "karma-mocha-reporter": "^2.2.5",
    "maptalks": "^1.0.0-rc.14",
    "mocha": "^9.0.3",
    "pixelmatch": "^4.0.2",
    "rollup": "^2.56.2",
    "rollup-plugin-terser": "^5.3.1"
  }
}
