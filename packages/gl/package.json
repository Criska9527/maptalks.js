{
  "name": "@maptalks/gl",
  "version": "0.2.0",
  "description": "WebGL infrastrcture for maptalks",
  "main": "dist/maptalksgl.js",
  "browser": "dist/maptalksgl.js",
  "module": "dist/maptalksgl.es.js",
  "jsnext:main": "dist/maptalksgl.es.js",
  "scripts": {
    "build": "npm run lint && cd ../reshader.gl && npm run build && cd ../gl && rollup --environment BUILD:production -c rollup.config.js",
    "build-dev": "cd ../reshader.gl && npm run build-dev && cd ../gl && rollup -c rollup.config.js",
    "dev": "cd ../reshader.gl && npm run build-dev && cd ../gl && rollup -w -c rollup.config.js",
    "test": "echo \"Error: no test specified\" && exit 1",
    "prepare": "npm run build",
    "lint": "eslint src/**/*.js",
    "preversion": "npm run lint"
  },
  "files": [
    "dist/maptalksgl.js",
    "dist/maptalksgl.es.js"
  ],
  "keywords": [
    "webgl",
    "maptalks"
  ],
  "author": "fuzhenn",
  "license": "UNLICENSED",
  "dependencies": {
    "@maptalks/fusiongl": "^0.1.0",
    "@maptalks/regl": "^1.3.11",
    "@maptalks/reshader.gl": "^0.2.0",
    "gl-matrix": "^2.6.1"
  },
  "devDependencies": {
    "@babel/core": "^7.2.2",
    "@babel/preset-env": "^7.2.3",
    "babel-eslint": "^10.0.1",
    "eslint": "^5.11.1",
    "eslint-config-maptalks": "^0.3.0",
    "rollup": "^1.0.1",
    "rollup-plugin-babel": "^4.2.0",
    "rollup-plugin-commonjs": "^9.2.0",
    "rollup-plugin-node-resolve": "^4.0.0",
    "rollup-plugin-terser": "^4.0.1",
    "rollup-plugin-uglify": "^4.0.0"
  }
}
