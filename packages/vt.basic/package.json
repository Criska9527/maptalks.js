{
  "name": "@maptalks/vt.basic",
  "version": "0.32.2",
  "description": "Basic rendering plugin for maptalks VectorTileLayer",
  "main": "dist/maptalks.vt.basic.js",
  "browser": "dist/maptalks.vt.basic.js",
  "scripts": {
    "build": "rollup --environment BUILD:production -c rollup.config.js",
    "build-dev": "rollup -c rollup.config.js",
    "dev": "rollup -w -c rollup.config.js",
    "fixtures": "npm run pretest && electron-mocha --require-main ./test/integration/env-fixtures.js  -t 10000 --renderer ./test/integration/*.spec.js",
    "lint": "eslint src/**/*.js test/**/*.js",
    "pretest": "npm run lint && cd ../vt && npm run build && cd ../vt.basic && npm run build",
    "preversion": "npm test",
    "test": "electron-mocha --require-main ./test/ignore-gpu-blacklist.js --renderer ./test/**/*.spec.js",
    "tdd": "electron-mocha --interactive --renderer ./test/**/*.spec.js",
    "prepare": "npm run build"
  },
  "files": [
    "dist/maptalks.vt.basic.js"
  ],
  "keywords": [
    "maptalks",
    "vector-tile"
  ],
  "author": "fuzhenn",
  "license": "UNLICENSED",
  "dependencies": {
    "@mapbox/point-geometry": "^0.1.0",
    "@maptalks/function-type": "^1.2.3",
    "@maptalks/gl": "<2.0.0",
    "@maptalks/vt-plugin": "^0.32.0",
    "color": "^3.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "@maptalks/rollup-plugin-glsl-minify": "^0.1.7",
    "@maptalks/vt": "^0.32.0",
    "babel-eslint": "^9.0.0",
    "electron": "^4.0.1",
    "electron-mocha": "^6.0.4",
    "eslint": "^4.19.1",
    "eslint-config-maptalks": "^0.3.0",
    "eslint-plugin-mocha": "^5.0.0",
    "maptalks": "1.0.0-alpha.12",
    "mocha": "^5.2.0",
    "pixelmatch": "^4.0.2",
    "rollup": "^1.0.1",
    "rollup-plugin-babel": "^4.2.0",
    "rollup-plugin-commonjs": "^9.2.0",
    "rollup-plugin-json": "^4.0.0",
    "rollup-plugin-node-resolve": "^4.0.0",
    "rollup-plugin-terser": "^4.0.1",
    "rollup-plugin-uglify": "^4.0.0"
  },
