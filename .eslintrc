{
  "root": true,
  "env": {
    "node": true,
    "browser": true,
    "es6": true
  },
  "extends": "eslint:recommended",
  "parser": "@babel/eslint-parser",
  "parserOptions": {
    "sourceType": "module"
  },
  "globals": {
    "importScripts": true,
    "globalThis": true,
    "Zlib": true
  },
  "rules": {
    "indent": "error",
    "no-var": "error"
  }
}
